      <script> 
         $(document).ready(function(){
            // jQuery methods go here...
              $(".login-form").submit(function(){
                  var radio_buttons = $("input[name='ch']");
                  if( radio_buttons.filter(':checked').length == 0){
                      $("#msg").text("No choice selected!").show().fadeOut(1500);
                    return false;
                  } else {
                    return true;
                 }
                  
              });
           
          });  
        
      </script>
  	
